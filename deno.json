{
    "name": "@velotype/velodesign",
    "license": "MIT",
    "version": "0.0.3",
    "exports": {
        ".": "./src/index.ts"
    },
    "publish": {
        "exclude": [
            "docs/",
            "tests/"
        ]
    },
    "tasks": {
        "test": "cd ./tests/ && deno run 'bundle*' && cd .. && deno test --allow-read --allow-net --allow-sys --allow-env --allow-write --allow-run",
        "test-manual": "cd ./tests/ && deno run 'bundle*' && cd .. && deno run --allow-read --allow-net --allow-sys ./tests/manual_server.ts"
    },
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "@velotype/velotype",
        "lib": ["dom", "deno.ns", "esnext"]
    },
    "imports": {
        "@astral/astral": "jsr:@astral/astral@^0.5.4",
        "@std/assert": "jsr:@std/assert@^1.0.16",
        "@std/testing": "jsr:@std/testing@^1.0.16",
        "@velotype/veloserver": "jsr:@velotype/veloserver@^0.0.2",
        "@velotype/velotype/jsx-runtime": "jsr:@velotype/velotype@^0.0.25",
        "@velotype/velotype": "jsr:@velotype/velotype@^0.0.25"
    }
  }